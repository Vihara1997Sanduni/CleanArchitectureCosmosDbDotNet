{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CosmosDb": "AccountEndpoint=https://localhost:8081/;AccountKey=<your-account-key>;ApiKind=Sql;ApplicationName=<your-app-name>"
  },
  "JwtSettings": {
    "Secret": "your-secret-key",
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "ExpiryMinutes": 60
  },
  "CosmosDb": {
    "ConnectionString": "your-cosmos-db-connection-string",
    "DatabaseName": "your-database-name"
  },
  "AllowedHosts": "*"
}
